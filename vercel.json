{
  "version": 2,
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "frontend/build",
  "env": {
    "NODE_ENV": "production",
    "CI": "false"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/index.html"
    }
  ]
}
